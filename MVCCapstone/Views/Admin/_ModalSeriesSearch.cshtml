<div class="modal fade" id="ForumIdSearch">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Series Id Search</h4>
            </div>
            <div class="modal-body">
                <div class="row" id="seriesSearch">
                    @using (Ajax.BeginForm("SeriesSearch", "Admin", new AjaxOptions { UpdateTargetId = "searchResult" }, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
                    {
                        <div class="form-group">
                            @Html.Label("Series Title", new { @class = "col-sm-3 control-label" })
                            <div class="col-sm-6">
                                @Html.TextBox("seriesTitle",null)
                            </div>
                            <div class="col-sm-3">
                                <input class="btn btn-primary" type="submit" value="Search" />
                            </div>
                        </div>
                       
                    }
                    <div id="searchResult">
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>